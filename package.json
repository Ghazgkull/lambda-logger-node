{
  "name": "@nike/lambda-logger-node",
  "version": "2.0.2",
  "description": "A logger middleware for AWS Lambda functions",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "style": "standard \"src/**/*.js\" | snazzy",
    "style:fix": "standard \"src/**/*.js\" --fix",
    "test": "npm run style && blue-tape test/**/**.js | ./node_modules/.bin/tap-spec",
    "test:watch": "npm run test -s & ./node_modules/.bin/chokidar 'src/**/*.js' 'test/**/*.js' --polling -c \"./node_modules/.bin/blue-tape test/**/**.js | node_modules/.bin/tap-spec\""
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.nike.net:NextGenPlatforms/lambda-logger-node.git"
  },
  "keywords": [
    "lambda",
    "logger"
  ],
  "author": "Tim Kye",
  "license": "UNLICENSED",
  "dependencies": {
    "object-assign": "^4.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "blue-tape": "^1.0.0",
    "chokidar-cli": "^1.2.0",
    "snazzy": "^7.0.0",
    "standard": "8.1.0",
    "tap-spec": "^4.1.1"
  }
}
